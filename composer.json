{
    "name": "voilab/csv",
    "version": "5.1.1",
    "type": "application",
    "description": "CSV parser that uses `fgetcsv` to parse file, string, streams or arrays, extract columns and provide per-column method to manipulate data.",
    "keywords": [
        "csv",
        "parser",
        "fgetcsv",
        "stream",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien",
            "email": "fabien@voilab.ch",
            "homepage": "https://www.voilab.ch",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "scrutinizer/ocular": "^1.5",
        "guzzlehttp/guzzle": "^6.5"
    }
}
